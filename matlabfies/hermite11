function  ypp = hermite11
  x1=[1 2 4];y=[2 4 3];
  L0=@(x) (x-2).*(x-4)/3;L1=@(x) (x-1).*(x-4)/(-2);L2=@(x) (x-1).*(x-2)/6;
  dL0=@(x) (2.*x-2-4)/3;dL1=@(x) (2.*x-1-4)/(-2);dL2=@(x) (2.*x-1-2)/6;
  x=1:.01:4;
  H1= (1-2.*(x-1)*4/3).*(L0(x)).^2.*y(1)+(1-2.*(x-2)*1/2).*(L1(x)).^2.*y(2)+(1-2.*(x-4)*5/6).*(L2(x)).^2.*y(3);
  H2=(x-1).*(L0(x)).^2+(x-2).*(L1(x)).^2+(x-4).*(L2(x)).^2;
  HH=H1+H2;
  plot(x,HH)
endfunction
